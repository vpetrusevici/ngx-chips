{
  "name": "@vpetrusevici/ngx-chips",
  "version": "2.2.5",
  "description": "Tag Input component for Angular",
  "scripts": {
    "release": "npm run build && ncp ./modules/core/styles ./dist/core/styles && cd dist && npm publish --access public",
    "build": "ng build lib",
    "server": "ng serve --port 3010",
    "start": "npm run server",
    "test": "ng test modules --browsers=ChromeHeadless --watch=false",
    "codecov": "ng test modules --code-coverage --browsers=ChromeHeadless --watch=false",
    "ng": "ng"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/animations": "~11.2.6",
    "@angular/cli": "~11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/language-service": "~11.2.6",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "zone.js": "~0.10.3"
  },
  "dependencies": {
    "@vpetrusevici/ng2-material-dropdown": ">=0.12.0"
  },
  "peerDependencies": {
    "@angular/animations": ">=11.0.0",
    "@angular/common": ">=11.0.0",
    "@angular/compiler": ">=11.0.0",
    "@angular/core": ">=11.0.0",
    "@angular/forms": ">=11.0.0"
  },
  "keywords": [
    "angular tag input",
    "tag input component",
    "material tag input",
    "angular chips component",
    "material chips"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Gbuomprisco/ngx-chips"
  },
  "bugs": {
    "url": "https://github.com/Gbuomprisco/ngx-chips/issues"
  },
  "homepage": "https://github.com/Gbuomprisco/ngx-chips",
  "ngPackage": {
    "lib": {
      "entryFile": "modules/index.ts"
    },
    "allowedNonPeerDependencies": [
      "@vpetrusevici/ng2-material-dropdown"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/angular@0.2.0"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
